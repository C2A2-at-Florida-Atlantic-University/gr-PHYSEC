    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [840, 8.0]
    rotation: 0
    state: true
- name: bitsPerSymbol
  id: variable
  parameters:
    comment: ''
    value: int(math.log(M,2))
